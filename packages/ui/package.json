    "eslint": "^7.32.0",
    "eslint-config-custom": "*",
    "jest": "^29.0.5",
    "jest-environment-jsdom": "^29.5.0",
    "jest-presets": "*",
    "postcss": "^8.4.21",
    "react": "^18.2.0",
    "tsconfig": "*",
    "tsup": "^6.2.3",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "react-dom": "^18.2.0"
