    "lint": "tsc --noEmit && eslint \"src/**/*.ts*\"",
    "test": "jest"
  },
  "jest": {
    "preset": "jest-presets/jest/node"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/node": "^18.11.9",
    "eslint": "^7.32.0",
    "eslint-config-custom": "*",
    "jest": "^26.6.3",
    "jest-presets": "*",
    "tsconfig": "*",
    "tsup": "^6.2.3",
    "typescript": "^4.8.3"
  }
}
